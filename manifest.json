{
  "manifest_version": 2,
  "name": "EZ Creds",
  "description": "A simple password vault",
  "version": "1.0",
  "author": "Anvita Ranka, Abhishek Mohanty, and Nanda Kishore",
  "browser_action": {
    "default_icon": "ez.png",
    "default_popup": "popup.html",
    "default_title": "EZ Creds"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}